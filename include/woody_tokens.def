#ifndef TOKEN
#define TOKEN(t)
#endif

#ifndef KEYWORD
#define KEYWORD(kw, string, length) TOKEN(kw)
#endif

KEYWORD(VAR, var, 3)
KEYWORD(FUNCTION, function, 8)
KEYWORD(RETURN, return, 6)
KEYWORD(END, end, 3)
KEYWORD(TRUE, true, 4)
KEYWORD(FALSE, false, 5)

TOKEN(COMMA)
TOKEN(PLUS)
TOKEN(MINUS)
TOKEN(ASTERIX)
TOKEN(SLASH)
TOKEN(EQ)
TOKEN(OPEN_PAREN)
TOKEN(CLOSE_PAREN)
TOKEN(NUMBER)
TOKEN(IDENTIFIER)
TOKEN(NEWLINE)
TOKEN(EOF)

#undef KEYWORD
#undef TOKEN
