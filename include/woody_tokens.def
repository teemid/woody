#ifndef TOKEN
#define TOKEN(t)
#endif

#ifndef KEYWORD
#define KEYWORD(kw) TOKEN(kw)
#endif

KEYWORD(VAR)
KEYWORD(FUNCTION)
KEYWORD(RETURN)
KEYWORD(END)
KEYWORD(TRUE)
KEYWORD(FALSE)

TOKEN(COMMA)
TOKEN(PLUS)
TOKEN(MINUS)
TOKEN(ASTERIX)
TOKEN(SLASH)
TOKEN(EQ)
TOKEN(OPEN_PAREN)
TOKEN(CLOSE_PAREN)
TOKEN(NUMBER)
TOKEN(IDENTIFIER)
TOKEN(NEWLINE)
TOKEN(EOF)

#undef KEYWORD
#undef TOKEN
